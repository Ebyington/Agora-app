{
  "name": "agora",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "16.17.1"
  }, 
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "apollo-server-express": "^3.11.1",
    "concurrently": "^5.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-scripts": "5.0.1",
    "babel-jest": "^26.6.0",
